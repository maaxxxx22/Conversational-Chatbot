<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">

    <!-- Get help selecting a career category -->
    <category>
        <pattern>get help career</pattern>
        <template>To assist you in exploring career paths in Computer Science, I'll ask you a few questions.
            <br/><br/>
            Are you passionate about design but find programming challenging?
            <button>
                <text>Yes</text>
                <postback>design yes</postback>
            </button>
            <button>
                <text>No</text>
                <postback>design no</postback>
            </button>
        </template>
    </category>

    <!-- Design Yes category -->
    <category>
        <pattern>design yes</pattern>
        <template>Have you worked with web applications or APIs before?
            <button>
                <text>Yes</text>
                <postback>web applications yes</postback>
            </button>
            <button>
                <text>No</text>
                <postback>web applications no</postback>
            </button>
        </template>
    </category>

    <!-- Design No category -->
    <category>
        <pattern>design no</pattern>
        <template>Would you rather work in a development environment?
            <button>
                <text>Yes</text>
                <postback>development yes</postback>
            </button>
            <button>
                <text>No</text>
                <postback>development no</postback>
            </button>
        </template>
    </category>

    <!-- Web Applications Yes category -->
    <category>
        <pattern>web applications yes</pattern>
        <template>Given your familiarity with web applications, you might find career opportunities in roles such as Web Developer or UX Designer.
            <br/>
            <br/>
            To learn more about Web Development, you can visit the<link><text>Web Development</text><url>https://en.wikipedia.org/wiki/Web_development</url>
        </link> Wikipedia page.
